PROJECT_NAME           = "Práctica Final: Rutas Aereas"
PROJECT_BRIEF          = "Tipos de datos abstractos: Imágenes"
PROJECT_LOGO           = @CMAKE_SOURCE_DIR@/@BASE_FOLDER@/doc/images/ed.png
HTML_HEADER            = @CMAKE_SOURCE_DIR@/@BASE_FOLDER@/doc/doxygen-awesome-css/header.html

OUTPUT_DIRECTORY       = @CMAKE_CURRENT_BINARY_DIR@/doc_doxygen/
INPUT                  = @CMAKE_SOURCE_DIR@/@BASE_FOLDER@/include @CMAKE_SOURCE_DIR@/@BASE_FOLDER@/src @CMAKE_SOURCE_DIR@/@BASE_FOLDER@/doc
GENERATE_LATEX         = NO
IMAGE_PATH             = @CMAKE_SOURCE_DIR@/@BASE_FOLDER@/doc/images
EXTRACT_PRIVATE        = NO

# Doxygen Awesome CSS needed options
GENERATE_TREEVIEW      = YES # optional. Also works without treeview
DISABLE_INDEX          = NO
FULL_SIDEBAR           = NO
HTML_EXTRA_STYLESHEET  = @CMAKE_SOURCE_DIR@/@BASE_FOLDER@/doc/doxygen-awesome-css/doxygen-awesome.css \
                        @CMAKE_SOURCE_DIR@/@BASE_FOLDER@/doc/doxygen-awesome-css/doxygen-awesome-sidebar-only.css \
                        @CMAKE_SOURCE_DIR@/@BASE_FOLDER@/doc/doxygen-awesome-css/doxygen-awesome-sidebar-only-darkmode-toggle.css
HTML_COLORSTYLE        = LIGHT # required with Doxygen >= 1.9.5
HTML_EXTRA_FILES       = @CMAKE_SOURCE_DIR@/@BASE_FOLDER@/doc/doxygen-awesome-css/doxygen-awesome-darkmode-toggle.js \
                        @CMAKE_SOURCE_DIR@/@BASE_FOLDER@/doc/doxygen-awesome-css/doxygen-awesome-fragment-copy-button.js \
                        @CMAKE_SOURCE_DIR@/@BASE_FOLDER@/doc/doxygen-awesome-css/doxygen-awesome-interactive-toc.js